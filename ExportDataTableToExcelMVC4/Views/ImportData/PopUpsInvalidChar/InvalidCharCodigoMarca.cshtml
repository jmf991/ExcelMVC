@model ExportDataTableToExcelInMVC4.Models.ImportarModel

<!--scripts de PopUps-->
<script>
    $(document).ready(function () {
        toastr.options = {
            "closeButton": true,
            "onclick": null,
            "showDuration": "0",
            "hideDuration": "0",
            "timeOut": "0",
            "showMethod": "fadeIn"
        };
            toastr.error('Error: (Caracteres Invalidos) una celda de la columna CodigoMarca tiene algun caracter inválido');
        });
</script>
<!--END scripts de los PopUps-->
<p>-Indice de la celda con <b>caracteres inválidos</b> de la columna <u>CodigoMarca</u>:</p>
@foreach (var index in ViewBag.invalidCharCodigoMarcaList)
    {
        <ul>
            <li>@index</li>
        </ul>
    }

  